.content-header
  .pull-left
    %h2 #{t('activerecord.models.venue')} - #{@venue}
  .pull-right
    .btn-group
      = link_to venues_path, class: 'btn', title: t('actions.back') do
        %i.icon-arrow-left
      = link_to edit_venue_path(@venue), class: 'btn', title: t('actions.edit') do
        %i.icon-edit
      = link_to new_venue_path, class: 'btn btn-success', title: "#{t('actions.create')} #{t('activerecord.models.venue').downcase}" do
        %i.icon-plus.icon-white
  .clearfix
  %ul.breadcrumb
    %li
      = link_to t('views.home'), root_path
      %span.divider /
    %li
      = link_to t('activerecord.models.venue').pluralize, venues_path
      %span.divider /
    %li.active #{t('activerecord.models.venue')} - #{@venue} 
  
.content-body
  %dt= t('activerecord.attributes.venue.name')
  %dd= @venue.name
  %dt= t('activerecord.attributes.venue.venue_id')
  %dd= @venue.venue_id
  %dt= t('activerecord.attributes.venue.deleted_at')
  %dd= @venue.deleted_at
  